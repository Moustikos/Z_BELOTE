{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.belote",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"score": {
				"uri": "model/Score.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": false,
			"phone": false
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.belote.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.belote.i18n.i18n"
				}
			},
			"scoreModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "score"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [
		        {
		          	"uri": "util/ClassFormatter.js"
		        }
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.belote.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "",
				"name": "Connection",
				"target": "Connection"
			}, {
				"pattern": "Play/{teamPath}",
				"name": "Play",
				"target": "Play"
			}, {
				"pattern": "Tables",
				"name": "Tables",
				"target": "Tables"
			}, {
				"pattern": "TableDetails",
				"name": "TableDetails",
				"target": "TableDetails"
			}],
			"targets": {
				"Connection": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Connection",
					"viewName": "Connection"
				},
				"Play": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Play",
					"viewName": "Play"
				},
				"Tables": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Tables",
					"viewName": "Tables"
					
				},
				"TableDetails": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "TableDetails",
					"clearControlAggregation": false,
					"viewId": "TableDetails"
					
				}
			}
		}
	}
}