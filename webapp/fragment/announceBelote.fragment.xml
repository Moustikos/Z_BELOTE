<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:card="sap.f.cards">
	<Dialog title="You can announce Belote !" contentWidth="400px" draggable="true" resizable="true" id="idBelotePopUp">
		<content>
			<Text text="Announce belote and rebelote ?"></Text>
		</content>
		<buttons>
			<Button text="Yes" press="onPressYesAnnounceBelote"/>
			<Button text="No" press="onPressNoAnnounceBelote"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>